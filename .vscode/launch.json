{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "Guess the number",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/cfsm-gen/examples/guess-number/Debug/guess-number",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceRoot}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description":  "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ]
    }
  ]
}